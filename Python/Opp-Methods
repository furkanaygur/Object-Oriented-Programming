# ************** Special Methods **************************

class Movie():
    def __init__(self, director, name, duration):
        self.director = director
        self.name = name
        self.duration = duration
        print("Movie Created with Movie __init__")

    def __str__(self):
        return f"This class Created from Coder {self.director}"

    def __len__(self):
        return self.duration


m = Movie("Furkan", "Movie", 123)
print(str(m))
print(f" Movie Duration = {len(m)}")
